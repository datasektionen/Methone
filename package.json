{
  "name": "methone",
  "version": "0.3.6",
  "description": "The worlds first Top-Bar-as-a-Service",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datasektionen/Methone.git"
  },
  "bugs": {
    "url": "https://github.com/datasektionen/Methone/issues"
  },
  "homepage": "https://methone.datasektionen.se/",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c && perl -i -0pe 's/\\ncreateSvgIcon\\(.*?;\\n//sg' dist/index.es.js dist/index.js",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "fuse.js": "^3.2.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rollup": "^0.62.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-prepack-up": "git+https://github.com/joseluisq/rollup-plugin-prepack.git",
    "rollup-plugin-url": "^1.4.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
