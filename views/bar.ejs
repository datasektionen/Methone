<style>
  html,
  body,
  div,
  form,
  fieldset,
  legend,
  label {
    margin: 0;
    padding: 0;
  }

  header {
    background-color: #212121;
    font-family: Lato, Arial;
    height: 50px;
  }

  #widther {
    max-width: 900px;
    margin: auto;
    position: relative;
  }

  button {
    position: relative;
    background-color: #FDDE00;
    border: none;
    font-size: 14px;
    height: 50px;
    padding: 0;
    verticala-align: top;
  }

  #superdelta {
    position: relative;
    background-color: #FDDE00;
    border: none;
    font-size: 14px;
    height: 50px;
    padding: 0;
    verticala-align: top;
    width: 50px;
    top: 0;
    display: inline-block;
    text-align: center;
    cursor: pointer;
  }

  #superdelta_img {
    margin-top: 10px;
    height: 31px;
    margin-left: 1.5px;
  }

  nav {
    display: inline-block;
    margin-left: 9px;
    vertical-align: top;
  }

  .navelement {
    display: inline-block;
    font-size: 15px;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    color: #f1f1f1 !important;
    text-decoration: none;
    padding: 17px 11px 16px 11px;
    cursor: pointer;
  }

  #login {
    position: relative;
    background-color: #FDDE00;
    border: none;
    font-size: 14px;
    height: 50px;
    padding: 0;
    verticala-align: top;
    width: 100px;
    font-family: Lato, Arial;
    text-transform: uppercase;
    cursor: pointer;
  }

  #leftside {
    display: inline-block;
    position: absolute;
    right: 0;
  }

  #login_text: {
    position: absolute;
    top: 17px;
    left: 0;
    width: 100%;
  }

  #startmenu {
    background-color: #212121;
    width: 300px;
    position: absolute;
    top:50px;
    display: none;
    opacity: 0;
  }
  #startmenu ul, #gobox ul {
    list-style: none;
    padding: 10px;

  }
  #startmenu li, #gobox li {
    padding: 10px;
    font-family: Lato, sans-serif;
  }
  #startmenu a , #gobox a {
    color: white;
    text-decoration: none;
    display: block;
  }
  #gobox {
    position: fixed;
    top:200px;
    left: 0;
    z-index: 9000;
    width: 95%;
    display: none;
  }
  #inner_gobox {
      background: #212121;
      width: 400px;
      margin: 0 auto;
      box-shadow: 0 5px 10px #CCC;
  }
  #gobox input {
    width: 360px;
    font-size: 28px;
    margin: 20px 20px 0 20px;
    padding: 5px 0;
    background: none;
    border: none;
    border-bottom: 2px solid rgba(255, 255, 255, 0.35);
    color: white;
    outline: none;
    text-transform: uppercase;
  }

</style>
