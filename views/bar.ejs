  }

  #gobox {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9006;
    width: 100%;
    height: 100%;
    display: none;
    background-color: rgba(0, 0, 0, 0.5);
    transition: all 200ms ease-out;
  }
  #inner_gobox {
      width: 600px;
      margin: 200px auto 0 auto;
      background: aliceblue;
      transition: all 2s ease-out;
      opacity: 0;
  }
  #input_wrap {
    background: #E91E63;
    box-shadow: 0 6px 15px rgba(0,0,0,0.4);
  }
  #gobox input {
    width: 560px;
    font-size: 28px;
    margin: 20px;
    padding: 5px 0;
    background: none;
    border: none;
    border-bottom: 2px solid rgba(255, 255, 255, 0.35);
    color: #FAFAFA;
    outline: none;
    text-transform: uppercase;
  }
  #gobox ul {
    list-style: none;
    padding: 0;
    margin: 0;
    transition: height 200ms ease-out;
    box-shadow: 0 6px 15px rgba(0,0,0,0.4);
  }
  #gobox li {
    padding: 10px;
    font-family: Lato, sans-serif;
  }
  #gobox a {
    color: white;
    text-decoration: none;
    display: block;
  }



  #startmenu {
    background-color: #212121;
    width: 100%;
    position: absolute;
    top:50px;
    display: none;
    opacity: 0;
    background-color: #FAFAFA;
    left: 0;
    width: 300px;
    box-shadow: 0 0 20px black;
    bottom: 0;
  }
  #startmenu ul {
    list-style: none;
    padding: 0;
    margin: 0;
  }
  #meth_tip_ux {
    position: absolute;
    bottom: 0px;
    font-size: 12px;
    left: -30px;
    text-decoration: none;
    color: #666666;
    transition: opacity 2s ease-out;
  }
